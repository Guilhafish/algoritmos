BEGIN MAIN_PROGRAM_NAME
        READ REAL num1 COMMA num2 COMMA num3 "Insira abaixo 3 números." 
        DEFINE REAL som SET 0 COMMA dif SET 0 COMMA prod SET 0 COMMA qdp SET 0
        EXECUTE som SET num1 SUM num2 SUM num3
        EXECUTE dif SET num1 SUB num2 SUB num3
        EXECUTE prod SET num1 MULT num2 MULT num3
        EXECUTE qdp SET ROUND_OPEN num1 MULT num2 ROUND_CLOSE DIV num3
        WRITE "soma: "  COMMA som  COMMA ";\ndiferença: "  COMMA dif  COMMA ";\nproduto: "  COMMA prod  COMMA ";\nquociente do produdo x1 e x2 pelo x3: "  COMMA qdp 
    END MAIN_PROGRAM_NAME