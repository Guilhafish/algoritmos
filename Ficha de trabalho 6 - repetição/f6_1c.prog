BEGIN MAIN_PROGRAM_NAME
        DEFINE INTEGER cont SET 0
        DEFINE INTEGER som SET 0
        DEFINE INTEGER prod SET 1
        WHILE cont LESS_THAN 16 
            WRITE "\n"  COMMA cont SUM 2 
            EXECUTE cont SET cont SUM 2
            EXECUTE som SET som SUM cont
            EXECUTE prod SET prod MULT cont
            WRITE "\nO dobro é: "  COMMA cont MULT 2  COMMA "\n" 
        END WHILE
        WRITE "\nA soma dos números\nde 1 a 16 é: "  COMMA som  COMMA ".\n" 
        WRITE "\nA multiplição dos números\nde 1 a 16 é: "  COMMA prod 
    END MAIN_PROGRAM_NAME