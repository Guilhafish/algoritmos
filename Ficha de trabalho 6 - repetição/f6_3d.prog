BEGIN MAIN_PROGRAM_NAME
        READ INTEGER N "Número de testes." 
        DEFINE INTEGER som SET 0
        DEFINE INTEGER med SET 0
        ITERATE INTEGER i FROM 0  TO N  STEP 1 
            READ INTEGER nota " Notas dos testes " 
            EXECUTE som SET som SUM nota
        END ITERATE
        EXECUTE med SET som DIV N
        WRITE "A média dos testes é de "  COMMA med 
    END MAIN_PROGRAM_NAME