BEGIN MAIN_PROGRAM_NAME
        DEFINE INTEGER som SET 0
        DEFINE INTEGER prod SET 1
        ITERATE INTEGER cont FROM 1  TO 20  STEP 1 
            EXECUTE cont SET cont SUM 1
            EXECUTE prod SET prod MULT cont
        END ITERATE
        WRITE "O produto dos números\nde 1 a 20 é "  COMMA prod  COMMA "." 
    END MAIN_PROGRAM_NAME