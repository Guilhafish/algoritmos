BEGIN MAIN_PROGRAM_NAME
        READ INTEGER NUM1 
        READ INTEGER NUM2 
        DEFINE INTEGER RESULT SET 0
        EXECUTE RESULT SET ROUND_OPEN NUM1 SUM NUM2 ROUND_CLOSE MULT NUM1
        WRITE "O resultado da soma de dois números inteiros\n" SUM "multiplicados pelo primeiro número\n" SUM "é igual a "  COMMA RESULT 
    END MAIN_PROGRAM_NAME