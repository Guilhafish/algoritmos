BEGIN MAIN_PROGRAM_NAME
        READ TEXT pecas "Código da peça?" 
        READ INTEGER valor "Valor da Peça comprada" 
        READ INTEGER quantidade "Quantidade de peças que vai comprar" 
        DEFINE INTEGER vtotal SET 0
        EXECUTE vtotal SET quantidade MULT valor
        WRITE "O código da peça é "  COMMA pecas  COMMA " e o valor total é "  COMMA vtotal  COMMA "." 
    END MAIN_PROGRAM_NAME