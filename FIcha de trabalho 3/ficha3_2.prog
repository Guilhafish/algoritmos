BEGIN MAIN_PROGRAM_NAME
        READ INTEGER teste1 COMMA teste2 COMMA teste3 "Nota dos testes?" 
        DEFINE INTEGER media SET 0
        EXECUTE media SET ROUND_OPEN teste1 SUM teste2 SUM teste3 ROUND_CLOSE DIV 3
        WRITE "A média dos três teste equivale a "  COMMA media 
    END MAIN_PROGRAM_NAME