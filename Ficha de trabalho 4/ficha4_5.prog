BEGIN MAIN_PROGRAM_NAME
        READ INTEGER sal "Escreva o seu salário mensal, (o percentual de reajuste é de 5%)." 
        DEFINE REAL sf SET 0.0
        EXECUTE sf SET sal MULT ROUND_OPEN 1 SUM 0.05 ROUND_CLOSE
        WRITE "O salário final depois do\npercentual de reajuste é "  COMMA sf  COMMA "€." 
    END MAIN_PROGRAM_NAME