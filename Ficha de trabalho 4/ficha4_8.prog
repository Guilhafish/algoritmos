BEGIN MAIN_PROGRAM_NAME
        READ INTEGER seg "Duração do Evento em segundos." 
        DEFINE INTEGER hr SET 0 COMMA minu SET 0
        EXECUTE hr SET seg DIV 3600
        EXECUTE minu SET seg DIV 60
        DEFINE INTEGER rs SET seg MOD 60 COMMA rm SET minu MOD 60
        WRITE "A duração do evento foi de:\n"  COMMA hr  COMMA "h"  COMMA rm  COMMA "m"  COMMA rs  COMMA "s" 
    END MAIN_PROGRAM_NAME