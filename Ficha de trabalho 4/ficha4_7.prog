BEGIN MAIN_PROGRAM_NAME
        READ REAL ncv COMMA vtsv COMMA sf COMMA vcv "Escreva:\n-O número de carros que vendeu (ncv);\n-O valor total de suas vendas (vtsv);\n-O seu salário fixo (sf);\n-O valor que recebe por carro vendido (vcv).\nPor essa ordem." 
        DEFINE REAL sfv SET 0.0 COMMA tv SET 0.0 COMMA vt SET 0.0 COMMA tp SET 0.0 COMMA p SET 0.0
        EXECUTE tv SET vcv MULT ncv
        EXECUTE tp SET ncv MULT 5
        EXECUTE p SET tp DIV 100
        EXECUTE vt SET tv MULT ROUND_OPEN 1 SUM p ROUND_CLOSE
        EXECUTE sfv SET sf SUM vt
        WRITE "O salário final do vendedor é "  COMMA sfv  COMMA "€." 
    END MAIN_PROGRAM_NAME