BEGIN MAIN_PROGRAM_NAME
        READ INTEGER neleitores COMMA vbranco COMMA vnulos "Escreva o número de eleitores, os votos brancos, votos nulos e os votos válidos, respetivamente." 
        DEFINE INTEGER vvalidos SET 0
        DEFINE REAL pvb SET 0.0 COMMA pvn SET 0.0 COMMA pvv SET 0.0
        EXECUTE vvalidos SET neleitores SUB ROUND_OPEN vbranco SUM vnulos ROUND_CLOSE
        EXECUTE pvb SET ROUND_OPEN 100 MULT vbranco ROUND_CLOSE DIV neleitores
        EXECUTE pvn SET ROUND_OPEN 100 MULT vnulos ROUND_CLOSE DIV neleitores
        EXECUTE pvv SET ROUND_OPEN 100 MULT vvalidos ROUND_CLOSE DIV neleitores
        WRITE neleitores  COMMA " pessoas foram votar, dessas "  COMMA neleitores  COMMA " pessoas:\n"  COMMA vbranco  COMMA " eram votos brancos ("  COMMA pvb  COMMA "%),\n"  COMMA vnulos  COMMA " eram votos nulos e ("  COMMA pvn  COMMA "%),\n"  COMMA vvalidos  COMMA " votos validos ("  COMMA pvv  COMMA "%)." 
    END MAIN_PROGRAM_NAME