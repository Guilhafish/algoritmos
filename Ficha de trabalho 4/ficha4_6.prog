BEGIN MAIN_PROGRAM_NAME
        READ REAL vfab "Escreva o valor de fábrica do carro." 
        DEFINE REAL cfin SET 0.0
        EXECUTE cfin SET vfab MULT ROUND_OPEN 1 SUM 0.73 ROUND_CLOSE
        WRITE "O custo final do carro ao consumidor é "  COMMA cfin  COMMA "€." 
    END MAIN_PROGRAM_NAME