BEGIN MAIN_PROGRAM_NAME
        READ INTEGER dia COMMA mes COMMA ano 
        DEFINE INTEGER me SET 0 COMMA an SET 0 COMMA d SET 0 COMMA tdia SET 0
        EXECUTE an SET ano MULT 365
        EXECUTE me SET mes MULT 30
        EXECUTE d SET ano MULT 1
        EXECUTE tdia SET an SUM me SUM d
        WRITE "Se voçê nasceu no dia "  COMMA dia  COMMA "/"  COMMA mes  COMMA "/"  COMMA ano  COMMA ",\nsignifica que está a "  COMMA tdia  COMMA " dias vivo." 
    END MAIN_PROGRAM_NAME