BEGIN MAIN_PROGRAM_NAME
        READ REAL test1 COMMA test2 "Escreva a nota de 2 testes." 
        DEFINE REAL med SET 0.0
        EXECUTE med SET ROUND_OPEN test1 SUM test2 ROUND_CLOSE DIV 2
        IF med GREATHER_THAN 10 
            WRITE "Você foi aprovado." 
        ELSE
            WRITE "Você não foi aprovado." 
        END IF
    END MAIN_PROGRAM_NAME