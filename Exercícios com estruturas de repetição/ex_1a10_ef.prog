BEGIN MAIN_PROGRAM_NAME
        DEFINE INTEGER cont SET 1
        DEFINE INTEGER som SET 0
        WHILE cont LESS_OR_EQUAL 10 
            EXECUTE som SET som SUM cont
            EXECUTE cont SET cont SUM 1
        END WHILE
        WRITE "A soma dos números de 1 a 10\né igual a "  COMMA som 
    END MAIN_PROGRAM_NAME