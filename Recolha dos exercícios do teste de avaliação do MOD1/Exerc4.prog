BEGIN MAIN_PROGRAM_NAME
        READ INTEGER numa "Escreva o número do aluno." 
        READ REAL trab1 COMMA trab2 COMMA trab3 "Notas obtidas em trabalhos." 
        READ REAL np "Nota de participação na aula." 
        DEFINE REAL MFINAL SET ROUND_OPEN trab1 MULT 0.65 SUM trab2 MULT 0.75 SUM trab3 MULT 0.85 SUM np ROUND_CLOSE DIV 4
        WRITE "\nA média final do aluno "  COMMA numa  COMMA " é "  COMMA MFINAL 
        IF MFINAL LESS_THAN 11 
            WRITE "\nO aluno vai fazer o exame." 
        ELSE
            WRITE "\nO aluno não vai fazer o  exame." 
        END IF
    END MAIN_PROGRAM_NAME