BEGIN MAIN_PROGRAM_NAME
        READ INTEGER num "Escreva um número." 
        DEFINE INTEGER cub SET num POWER 3
        DEFINE INTEGER caq3 SET ROUND_OPEN num POWER 2 ROUND_CLOSE SUM 3
        WRITE "O cubo do número é "  COMMA cub  COMMA "\nO quadrado acrescido de 3 unidades é "  COMMA caq3 
    END MAIN_PROGRAM_NAME