BEGIN MAIN_PROGRAM_NAME
        READ INTEGER valor1 COMMA valor2 COMMA valor3 "Escreva 3 valores" 
        DEFINE INTEGER som SET valor1 SUM valor2
        DEFINE INTEGER prod SET valor2 MULT valor3
        DEFINE INTEGER med SET ROUND_OPEN valor1 SUM valor2 SUM valor3 ROUND_CLOSE DIV 3
        WRITE "A soma dos 2 primeiros é "  COMMA som  COMMA "\nO produto dos 2 últimos é "  COMMA prod  COMMA "\nA média de todos é "  COMMA med 
    END MAIN_PROGRAM_NAME