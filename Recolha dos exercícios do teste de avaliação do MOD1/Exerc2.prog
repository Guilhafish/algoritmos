BEGIN MAIN_PROGRAM_NAME
        READ INTEGER X COMMA Y 
        DEFINE INTEGER var1 SET X MOD 2
        IF var1 EQUAL 0 
            WRITE "O número é par" 
            DEFINE INTEGER mult SET X MULT Y
            WRITE "\nA multiplicação de X com Y é "  COMMA mult 
        ELSE
            WRITE "O número não é par" 
            DEFINE INTEGER sub SET X SUB Y
            WRITE "\nA subtração de X com Y é "  COMMA sub 
        END IF
    END MAIN_PROGRAM_NAME