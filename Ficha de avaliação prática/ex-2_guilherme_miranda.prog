BEGIN MAIN_PROGRAM_NAME
        READ REAL nh COMMA ch COMMA td "Escreva o número de horas trabalhadas (nh);\nO custo de hora trabalhada (ch);\nA taxa percentual de desconto (td)." 
        DEFINE REAL st SET 0 COMMA vd SET 0 COMMA sl SET 0
        EXECUTE st SET nh MULT ch
        EXECUTE vd SET ROUND_OPEN td DIV 100 ROUND_CLOSE MULT st
        EXECUTE sl SET st SUB vd
        WRITE "Ele trabalhou "  COMMA nh  COMMA " horas;\no salário bruto é igual a "  COMMA st  COMMA "€;\nele teve um desconto de "  COMMA vd  COMMA "€;\no salário líquido é igual a "  COMMA sl  COMMA "€." 
    END MAIN_PROGRAM_NAME