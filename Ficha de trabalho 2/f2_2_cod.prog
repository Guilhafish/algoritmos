BEGIN MAIN_PROGRAM_NAME
        READ INTEGER raio 
        DEFINE REAL p SET 0.0
        DEFINE REAL a SET 0.0
        EXECUTE p SET 2 MULT CONST_PI MULT raio
        EXECUTE a SET CONST_PI MULT raio POWER 2
        WRITE "Perimetro: "  COMMA p  COMMA "\nArea: "  COMMA a 
    END MAIN_PROGRAM_NAME